msgid "plugins.generic.mailSendFilter.name"
msgstr "Filter za slanje e-pošte"

msgid "plugins.generic.mailSendFilter.description"
msgstr "Ovaj dodatak omogućava izbjegavanje slanja e-pošte na određene korisničke račune prema specifičnim postavkama i pravilima, korisno za smanjenje povratnih poruka i sprječavanje dodavanja vašeg mail servera na crnu listu."

msgid "plugins.generic.mailSendFilter.downloadEmails"
msgstr "Preuzmi blokirane e-poruke"

msgid "plugins.generic.mailSendFilter.general"
msgstr "Opće postavke"

msgid "plugins.generic.mailSendFilter.checkInactivity"
msgstr "Preskače neaktivne korisničke račune na osnovu posljednje prijave, prema donjim pravilima praga."

msgid "plugins.generic.mailSendFilter.checkMxRecord"
msgstr "Preskače e-poštne domene bez valjanog MX zapisa."

msgid "plugins.generic.mailSendFilter.checkDisposable"
msgstr "Preskače e-poštu koja pripada jednokratnim uslugama."

msgid "plugins.generic.mailSendFilter.checkNeverLoggedIn"
msgstr "Preskače račune koji se nikada nisu prijavili."

msgid "plugins.generic.mailSendFilter.checkNotValidated"
msgstr "Preskače nepotvrđene korisničke račune."

msgid "plugins.generic.mailSendFilter.disposableDomainsUrl"
msgstr "Postavlja URL za preuzimanje liste jednokratnih domena. <b>Stavke moraju biti odvojene novim redovima</b>."

msgid "plugins.generic.mailSendFilter.disposableDomainsExpiration"
msgstr "Postavlja rok trajanja jednokratnih domena u danima."

msgid "plugins.generic.mailSendFilter.passthroughMailKeys"
msgstr "Postavlja ključeve e-pošte koji zaobilaze sve provjere."

msgid "plugins.generic.mailSendFilter.inactivity.threshold.days"
msgstr "Provjerava račune starije od navedenog broja dana za neaktivnost (posebno korisno za opciju 'nikad prijavljeni')."

msgid "plugins.generic.mailSendFilter.inactivity.threshold.rules"
msgstr "Pravila praga neaktivnosti (u danima)."

msgid "plugins.generic.mailSendFilter.inactivity.threshold.rules.help"
msgstr "<ul><li>Ostavite polje prazno da ne vršite provjeru za navedenu ulogu</li><li>Postavljanje na 0 znači da uloga nikada neće biti smatrana neaktivnom</li><li>Ako korisnik odgovara više pravila, koristit će se najpopustljivije pravilo</li></ul>"

msgid "user.role.none"
msgstr "Korisnički računi bez dodijeljene uloge"

msgid "user.with.submission"
msgstr "Korisnički računi s barem jednom prijavom"

msgid "plugins.generic.mailSendFilter.reason.disposableService"
msgstr "Jednokratna e-poštna usluga"

msgid "plugins.generic.mailSendFilter.reason.invalidMailExchange"
msgstr "Nevažeći MX zapis"

msgid "plugins.generic.mailSendFilter.reason.notValidated"
msgstr "E-pošta nije potvrđena"

msgid "plugins.generic.mailSendFilter.reason.never_logged"
msgstr "Korisnik se nikada nije prijavio"

msgid "plugins.generic.mailSendFilter.reason.inactive"
msgstr "Neaktivan račun"

msgid "plugins.generic.mailSendFilter.limitedActivation"
msgstr "Samo administrator stranice može aktivirati/deaktivirati ovaj dodatak"
