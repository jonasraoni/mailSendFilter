msgid "plugins.generic.mailSendFilter.name"
msgstr "پلگینی فڵتری ناردنی ئیمەیل"

msgid "plugins.generic.mailSendFilter.description"
msgstr "ئەم پلگینە ڕێگەدەدات لە ناردنی ئیمەیل بۆ هەندێک هەژماری بەکارهێنەر ڕابگرین، بەپێی ڕێکخستن و یاساکانی دیاریکراو، بۆ کەمکردنەوەی ئیمەیلە گەڕاوەکان و ڕێگریکردن لە زیادبوونی سێرڤەری ئیمەیلەکەت بۆ لیستی ڕاگرتن."

msgid "plugins.generic.mailSendFilter.downloadEmails"
msgstr "داگرتنی ئیمەیلە بلۆککراوەکان"

msgid "plugins.generic.mailSendFilter.general"
msgstr "ڕێکخستنە گشتییەکان"

msgid "plugins.generic.mailSendFilter.checkInactivity"
msgstr "هەژمارەکانی بەکارهێنەری ناچالاک بلۆک دەکات بەپێی کاتی چوونەژوورەوەی دووا، بەپێی یاساکانی لەرەلەری خوارەوە."

msgid "plugins.generic.mailSendFilter.checkMxRecord"
msgstr "دۆمەینەکانی ئیمەیل بەبێ تۆمارێکی MX دروست بلۆک دەکات."

msgid "plugins.generic.mailSendFilter.checkDisposable"
msgstr "ئیمەیلەکانی خزمەتگوزاریەکانی ئیمەیلی یەکجارەکی بلۆک دەکات."

msgid "plugins.generic.mailSendFilter.checkNeverLoggedIn"
msgstr "هەژمارەکانی بەکارهێنەری کە هەرگیز چوونەتەژوورەوە بلۆک دەکات."

msgid "plugins.generic.mailSendFilter.checkNotValidated"
msgstr "هەژمارەکانی بەکارهێنەری پشتڕاستنەکراو بلۆک دەکات."

msgid "plugins.generic.mailSendFilter.disposableDomainsUrl"
msgstr "ناونیشانی URLی لیستی دۆمەینە یەکجارەکەکان دادەنێت. <b>پێویستە ئایتمەکان بە هێڵی نوێ جیا بکرێنەوە</b>."

msgid "plugins.generic.mailSendFilter.disposableDomainsExpiration"
msgstr "ماوەی بەسەرچوونی دۆمەینە یەکجارەکەکان دادەنێت بە ڕۆژ."

msgid "plugins.generic.mailSendFilter.passthroughMailKeys"
msgstr "کلیلەکانی ئیمەیل کە هەموو پشکنینەکان ڕێگەبدەن دادەنێت."

msgid "plugins.generic.mailSendFilter.inactivity.threshold.days"
msgstr "هەژمارە کۆنەکانی زیاتر لە ڕۆژە دیاریکراوەکە چاودێری دەکات بۆ ناچالاکی (بەتایبەتی بۆ هەڵبژاردەی 'هەژمارەکانی کە هەرگیز چوونەتەژوورەوە')."

msgid "plugins.generic.mailSendFilter.inactivity.threshold.rules"
msgstr "یاساکانی لەرەلەری ناچالاکی بە ڕۆژ."

msgid "plugins.generic.mailSendFilter.inactivity.threshold.rules.help"
msgstr "<ul><li>بۆ نەکردنی پشکنین بۆ ڕۆڵی دیاریکراو فیلادەکە بەتاڵ بەجێبهێڵە</li><li>دانانی فیلادەکە بە 0 واتای ئەوەیە کە ڕۆڵەکە هەرگیز وەک ناچالاک ناژمێردرێت</li><li>ئەگەر بەکارهێنەرێک گونجاو بێت لەگەڵ چەند یاسایەک، یاسای زۆرترین ڕێگەدان بەکاردەهێنرێت بۆ دیاریکردنی ناچالاکی</li></ul>"

msgid "user.role.none"
msgstr "هەژمارەکانی بەکارهێنەری بێ هیچ ڕۆڵێک"

msgid "user.with.submission"
msgstr "هەژمارەکانی بەکارهێنەری بەلایەنی کەم یەک ناردنیان هەیە"

msgid "plugins.generic.mailSendFilter.reason.disposableService"
msgstr "خزمەتگوزاری ئیمەیلی یەکجارە"

msgid "plugins.generic.mailSendFilter.reason.invalidMailExchange"
msgstr "تۆماری MX نادروست"

msgid "plugins.generic.mailSendFilter.reason.notValidated"
msgstr "ئیمەیل پشتڕاستنەکراوە"

msgid "plugins.generic.mailSendFilter.reason.never_logged"
msgstr "بەکارهێنەر هەرگیز چوونەتەژوورەوە"

msgid "plugins.generic.mailSendFilter.reason.inactive"
msgstr "هەژماری ناچالاک"

msgid "plugins.generic.mailSendFilter.limitedActivation"
msgstr "تەنها بەڕێوەبەری ماڵپەڕ دەتوانێت ئەم پلگینە چالاک یان ناچالاک بکات"
